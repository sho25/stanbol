        <rdfs:label xml:lang="en">affectsWidget</rdfs:label>
        <rdfs:range rdf:resource="&interaction;UIElement"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


